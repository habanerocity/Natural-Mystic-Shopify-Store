{% if paginate.pages > 1 %}
    <nav aria-label="Page navigation">
        <ul class="pagination justify-content-center">
            {% if paginate.previous.is_link %}
                <li class="page-item">
                    <a class="page-link dot" href="{{ paginate.previous.url }}">{{ paginate.previous.title }}</a>
                </li>
            {% endif %}
            {% for part in paginate.parts %}
                {% if part.is_link %}
                    <li class="page-item">
                        <a class="page-link dot" href="{{ part.url }}">{{ part.title }}</a>
                    </li>
                {% else %}
                    <li class="page-item disabled">
                        <a class="page-link" href="{{ part.url }}">{{ part.title }}</a>
                    </li>

                {% endif %}
            {% endfor %}
            {% if paginate.next.is_link %}
                <li class="page-item ">
                    <a class="page-link dot" href="{{ paginate.next.url }}">{{ paginate.next.title }}</a>
                </li>
            {% endif %}
        </ul>
    </nav>
{% endif %}